<?xml version="1.0" encoding="UTF-8"?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:local="clr-namespace:FormsSample"
             xmlns:master="clr-namespace:FormsSample.Pages.MasterPage"
             x:Class="FormsSample.Pages.MasterPage.RootPage">


    <MasterDetailPage.Master>
        <master:MasterPage x:Name="master"></master:MasterPage>
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:FormsSamplePage x:Name="detail"></local:FormsSamplePage>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
    
</MasterDetailPage>
